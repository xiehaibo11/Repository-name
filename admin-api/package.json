{
  "name": "backend-management",
  "version": "1.0.0",
  "description": "后端管理系统",
  "main": "src/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:safe": "node scripts/start-dev.js",
    "dev:full": "concurrently \"npm run redis:start\" \"npm run dev\"",
    "dev:with-redis": "node scripts/start-with-redis.js",
    "prod": "node scripts/start-prod.js",
    "build": "tsc",
    "test": "jest",
    "kill-port": "node -e \"require('./src/utils/portManager.ts').developmentPortCleaner(3001)\"",
    "redis:start": "redis-server redis-dev.conf",
    "redis:stop": "redis-cli -a 123456 shutdown",
    "redis:status": "redis-cli -a 123456 ping",
    "security-check": "node scripts/security-check.js",
    "predeploy": "npm run security-check && npm run build",
    "init-avoid-win": "ts-node scripts/initSuperJackpot.ts",
    "avoid-win:status": "ts-node -e \"import('./scripts/initSuperJackpot').then(m => m.initializeAvoidWinSystem())\""
  },
  "keywords": [
    "backend",
    "management",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/redis": "^4.0.10",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.12.0",
    "redis": "^5.6.0",
    "sequelize": "^6.37.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.14",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.0",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
